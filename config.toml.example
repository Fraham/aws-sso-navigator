# AWS SSO Navigator Configuration
# Place this file at ~/.config/aws-sso-navigator/config.toml

# Default values to pre-select (optional)
default_client = "myclient"
default_account = "dev"
default_role = "admin"

# Use unified mode by default (optional)
unified_mode = false

# Set selected profile as default (optional)
set_default = false

# List profiles without selection (optional)
list = false

# Show recent profiles first (optional)
recent = false

# Maximum number of recent profiles to keep (optional)
max_recent_profiles = 100

# Custom AWS config path (optional)
# aws_config_path = "/path/to/custom/config"